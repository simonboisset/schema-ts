{
  "name": "validest",
  "version": "0.6.1",
  "description": "Typed schema validation",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rm -rf dist && builder && tsc",
    "version:major": "yarn version --new-version major --no-git-tag-version",
    "version:minor": "yarn version --new-version minor --no-git-tag-version",
    "version:patch": "yarn version --new-version patch --no-git-tag-version",
    "publish:lib": "yarn publish --access public"
  },
  "repository": { "type": "git", "url": "https://github.com/simonboisset/validest", "directory": "packages/validest" },
  "bugs": { "url": "https://github.com/simonboisset/validest/issues" },
  "homepage": "https://github.com/simonboisset/validest",
  "author": "Simon Boisset <simonboisset.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@validest/and": "0.6.1",
    "@validest/any": "0.6.1",
    "@validest/array": "0.6.1",
    "@validest/core": "0.6.1",
    "@validest/date": "0.6.1",
    "@validest/empty": "0.6.1",
    "@validest/int": "0.6.1",
    "@validest/json": "0.6.1",
    "@validest/nullable": "0.6.1",
    "@validest/number": "0.6.1",
    "@validest/object": "0.6.1",
    "@validest/one-of": "0.6.1",
    "@validest/optional": "0.6.1",
    "@validest/or": "0.6.1",
    "@validest/string": "0.6.1"
  },
  "devDependencies": {
    "builder": "*",
    "esbuild": "^0.14.42",
    "esbuild-node-externals": "^1.4.1",
    "typescript": "^4.7.4"
  },
  "keywords": ["typescript", "validation", "schema"]
}
