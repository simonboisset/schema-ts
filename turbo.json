{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "publish:lib": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
