{
  "name": "@validest/remix-react",
  "version": "0.6.1",
  "description": "Typed schema validation for remix stack",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rm -rf dist && builder && tsc",
    "version:major": "yarn version --new-version major --no-git-tag-version",
    "version:minor": "yarn version --new-version minor --no-git-tag-version",
    "version:patch": "yarn version --new-version patch --no-git-tag-version",
    "publish:lib": "yarn publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simonboisset/validest",
    "directory": "packages/remix-react"
  },
  "bugs": { "url": "https://github.com/simonboisset/validest/issues" },
  "homepage": "https://github.com/simonboisset/validest",
  "author": "Simon Boisset <simonboisset.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@validest/core": "*",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "builder": "*",
    "esbuild": "^0.14.42",
    "esbuild-node-externals": "^1.4.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": { "@remix-run/node": "^1.5.1", "@remix-run/react": "^1.5.1", "react": ">=16" },
  "dependencies": { "@validest/form": "0.6.1" },
  "keywords": [
    "typescript",
    "react",
    "remix",
    "form",
    "validation",
    "schema",
    "form-validation",
    "remix-form",
    "remix-form-validation"
  ]
